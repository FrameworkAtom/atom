{
    "name": "frameworkatom/atom",
	"type": "project",
    "description": "The Atom Framework",
	"version": "1.0.0",
	"keywords": [
		"framework",
		"atom"
	],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "require": {
		"php": "^7.1.3",
		"frameworkatom/framework": "^1.0.0",
    	"frameworkatom/console": "*"
	},
	"config": {
		"optimize-autoloader": true,
		"prefered-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
    "prefer-stable": true
}
